  "scripts": {
    "build": "tsup src/index.ts --minify --dts --format cjs --out-dir dist",
    "start:prod": "node dist/index.cjs",
    "dev": "tsx watch src/index.ts",
    "start:migrate:prod": "npx prisma migrate deploy && npm run start:prod"
